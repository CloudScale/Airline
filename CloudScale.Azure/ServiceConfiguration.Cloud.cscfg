<?xml version="1.0" encoding="utf-8"?>
<ServiceConfiguration serviceName="CloudScale.Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceConfiguration" osFamily="4" osVersion="*" schemaVersion="2014-01.2.3">
  <Role name="CloudScale.Web">
    <Instances count="1" />
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" value="UseDevelopmentStorage=true" />
      <Setting name="Microsoft.ServiceBus.ConnectionString" value="" />
      <Setting name="WebHost" value="http://movies.devopsnirvana.com/" />
      <Setting name="ApiHost" value="http://api.movies.devopsnirvana.com/" />
      <Setting name="SignalRHost" value="http://signalr.movies.devopsnirvana.com/" />
      <Setting name="MoviesDataContext" value="Data Source=(local);Initial Catalog=CloudScaleMovies;Integrated Security=True" />
    </ConfigurationSettings>
  </Role>
</ServiceConfiguration>